<project>
    <pomVersion>3</pomVersion>
    <extend>${basedir}/../../etc/project.xml</extend>

    <name>Geronimo :: Java Server Pages Specification</name>
    <groupId>geronimo-spec</groupId>
    <id>geronimo-spec-jsp</id>
    <shortDescription>
    </shortDescription>
    <description>
    </description>
    <siteDirectory></siteDirectory>
    <distributionDirectory></distributionDirectory>

    <package>javax.servlet.jsp</package>
    <currentVersion>DEV</currentVersion>

    <!-- ============ -->
    <!-- Dependencies -->
    <!-- ============ -->

    <dependencies>

        <!-- Module Dependencies -->

        <dependency>
            <groupId>geronimo-spec</groupId>
            <artifactId>geronimo-spec-servlet</artifactId>
            <version>DEV</version>
            <properties>
                <module>true</module>
            </properties>
        </dependency>

    </dependencies>

    <build>
        <resources>
            <resource>
                <directory>${basedir}/src/schema</directory>
                <targetPath>javax/servlet/jsp/resources</targetPath>
                <includes>
                    <include>*.dtd</include>
                    <include>*.xsd</include>
                </includes>
            </resource>
        </resources>
    </build>
</project>
