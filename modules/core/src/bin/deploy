DIRNAME=`dirname "$0"`
COMMAND_PATH="geronimo/deploy"
export JAVA_OPTS="$JAVA_OPTS -ea"

exec "$DIRNAME/twiddle" $TWIDDLE_OPTS "$COMMAND_PATH" -- "$@"
