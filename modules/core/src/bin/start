PROGNAME=`basename $0`
DIRNAME=`dirname $0`

COMMAND_PATH="geronimo/start"

if [ -n "$TWIDDLE_OPTS" ]; then
    TWIDDLE_OPTS=""
fi

exec $DIRNAME/twiddle $TWIDDLE_OPTS $COMMAND_PATH -- "$@"
